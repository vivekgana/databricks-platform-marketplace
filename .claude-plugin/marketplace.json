  "requirements": {
    "claude_version": ">=3.5",
    "python_version": ">=3.10",
    "dependencies": {
      "databricks-sdk": ">=0.20.0",
      "delta-spark": ">=3.0.0",
      "mlflow": ">=2.10.0",
      "great-expectations": ">=0.18.0",
      "pyspark": ">=3.5.0"
    },
    "system_packages": [
      "git",
      "docker"
    ]
  },
  "plugins": [
    {
      "name": "databricks-engineering",
      "path": "./plugins/databricks-engineering",
      "featured": true,
      "required": true
    },
    {
      "name": "databricks-mlops",
      "path": "./plugins/databricks-mlops",
      "featured": true,
      "required": false
    },
    {
      "name": "databricks-governance",
      "path": "./plugins/databricks-governance",
      "featured": false,
      "required": false
    }
  ],
  "installation": {
    "quick_start": "npx claude-plugins install @vivekgana/databricks-platform/databricks-engineering",
    "manual_install": [
      "/plugin marketplace add https://github.com/vivekgana/databricks-platform-marketplace",
      "/plugin install databricks-engineering"
    ]
  },
  "configuration": {
    "required_env_vars": [
      "DATABRICKS_HOST",
      "DATABRICKS_TOKEN"
    ],
    "optional_env_vars": [
      "DATABRICKS_WAREHOUSE_ID",
      "DATABRICKS_CLUSTER_ID"
    ],
    "config_file": ".databricks-plugin-config.yaml"
  },
